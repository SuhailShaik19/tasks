{
  "compilerOptions": {
   "target": "ES6",
    "module": "CommonJS",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "outDir": "dist",
    "rootDir": "src",
    "skipLibCheck": true,
    "types": ["node"],
    "allowSyntheticDefaultImports": true
  },
   "include": ["src/**/*.ts"]
}










